@article{Wang.2002,
 abstract = {Notes that consumers who compare products often evaluate products more favorably when the products have unique positive features but common negative features than when they have unique negative features but common positive ones. Two studies (Exp 1 and Exp 2, n=64) examined contingencies in the occurrence of these {\textquotedbl}cancellation{\textquotedbl} effects and explored their implications for people's evaluations of the products being compared and other products they encounter subsequently. Some subjects (Ss) were explicitly told to compare 2 products at the time they received information about them. They not only disregarded the products' common features when initially evaluating these products, but disregarded evaluatively similar features of a 3rd product they encountered some time later. Other Ss were initially told to describe each product separately. In this condition, they formed an overall impression of each product individually, and so cancellation effects were not apparent. Nevertheless, their preference for one product over the other was based on a comparison of the products' individual features that they could recall at the time of judgment rather than on the global impressions they had formed when the product information was first presented.},
 author = {Wang, Jing and {Wyer Jr}, Robert S},
 year = {2002},
 title = {Comparative judgment processes: The effects of task objectives and time delay on product evaluations},
 pages = {327--340},
 volume = {12},
 number = {4},
