 issn = {0974-9861},
 journal = {Psychological Studies},
 doi = {\url{10.1007/s12646-016-0358-6}}
}


@article{Jang.2016,
 abstract = {The current research proposes that the extent to which consumer choices are

affected by the decision process depends on the underlying nature of the choice

problem. Specifically, choices resulting from substantial inter-brand comparisons and

tradeoff analyses are vulnerable to whether product information is evaluated by

attribute- or alternative-based processing. By contrast, choices resulting from a minimal

cognitive processing are less sensitive to variations in the decision strategy employed.

We test our theory in the well-known domain of choice context effects. Across three

studies using multiple operationalizations of the decision process (i.e., information

display format, product presentation mode, and processing goal), we find converging

evidence that the more cognitively involving compromise choice increases when the

environment facilitates attribute- versus alternative-based processing. Conversely, the

choice of asymmetrically dominating option, which is characterized by relatively little

analytical processing, does not depend on the type of decision strategy highlighted by

the task.},
 author = {Jang, Jung Min and Yoon, Song Oh},
 year = {2016},
 title = {The effect of attribute-based and alternative-based processing on consumer choice in context},
 url = {\url{http://dx.doi.org/10.1007/s11002-014-9346-6}},
 pages = {511--524},
 volume = {27},
 number = {3},
