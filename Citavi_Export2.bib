@misc{Chrzan.2000,
 abstract = {his paper compares four design strategies for choice-based experiments: catalog-based designs for full-profile experiments, recipe-based designs for partial profile experiments, computer optimized designs using SAS OPTEX software; and randomized designs using CBC software. The authors (Chrzan and Orme) compare these strategies in terms of design efficiency and their ability to capture particular effects (main, cross- and alternative-specific effects, and interactions).



