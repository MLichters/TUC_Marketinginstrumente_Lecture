 pages = {335--342},
 volume = {67},
 number = {3},
 journal = {Journal of clinical epidemiology},
 doi = {\url{10.1016/j.jclinepi.2013.09.009}},
 file = {Eekhout, Vet et al. 2014 - Missing data in a multi-item:Attachments/Eekhout, Vet et al. 2014 - Missing data in a multi-item.pdf:application/pdf}
